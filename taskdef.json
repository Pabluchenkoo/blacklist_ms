{
    "executionRoleArn": "arn:aws:iam::682033463843:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "Container-app-python",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 5000,
                    "protocol": "tcp",
                    "containerPort": 5000
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "512",
    "memory": "1024",
    "family": "Task-app-python"
}